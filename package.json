{
  "name": "ember-lifeline-codemods",
  "version": "0.1.0",
  "scripts": {
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "test:watch": "codemod-cli test --watch",
    "lint": "eslint .",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "bin": "./bin/cli.js",
  "keywords": [
    "codemod-cli"
  ],
  "dependencies": {
    "codemod-cli": "^2.0.0",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "engines": {
    "node": "8.* || 10.* || >= 12"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 110,
    "trailingComma": "all"
  }
}
